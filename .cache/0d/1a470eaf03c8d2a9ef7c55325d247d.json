  height: 30px;\r\n    width: 30px;\r\n    margin: 10px;\r\n    outline: 0;\r\n}\r\n\r\n.rotate {\r\n    animation: rotate .5s;\r\n}\r\n\r\n.touch {\r\n    animation: touch .25s linear;\r\n}\r\n\r\n.swipe-right {\r\n    animation: swipe-right .5s linear;\r\n}\r\n\r\n.swipe-left {\r\n    animation: swipe-left .5s linear;\r\n}\r\n\r\n.slide {\r\n    animation: slide .5s;\r\n}\r\n\r\n@keyframes touch {\r\n    from {transform: scale(.9);}\r\n    to {transform: scale(1);}\r\n}\r\n\r\n@keyframes slide {\r\n    from {transform: translate(100px);}\r\n    to {transform: translate(0);}\r\n}\r\n\r\n@keyframes swipe-right {\r\n    from {transform: translate(0);}\r\n    to {transform: translate(999px);}\r\n}\r\n\r\n@keyframes swipe-left {\r\n    from {transform: translate(0);}\r\n    to {transform: translate(-999px);}\r\n}\r\n\r\n@keyframes rotate {\r\n    from {transform: rotate(90deg);}\r\n    to {transform: rotate(0);}\r\n}\r\n\r\n@media screen and (max-width: 650px) {\r\n    .main {\r\n        width: 98%;\r\n        height: 100vh;\r\n    }\r\n    header > h1 {\r\n        font-size: 24px;\r\n    }\r\n    .text {\r\n        font-size: 13px;\r\n    }\r\n}\r\n\r\n@media screen and (max-width: 400px) {\r\n    .tweet {\r\n        padding: 5px;\r\n    }\r\n    .text {\r\n        font-size: 12px;\r\n    }\r\n    .screen_name {\r\n        font-size: 14px;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"97289d2e085042b82f7a17dd182af754","cacheData":{"env":{}}}